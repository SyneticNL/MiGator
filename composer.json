{
    "name": "synetic/migator",
    "description": "Laravel schema migration generator.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "synetic",
        "laravel",
        "migator",
        "migrations"
    ],
    "autoload": {
        "psr-4": {
            "Synetic\\Migator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jeffrey Bertoen",
            "email": "jeffrey@synetic.nl"
        },
        {
            "name": "Tom van Etten",
            "email": "tom.vanetten@synetic.nl"
        },
        {
            "name": "Aubrey Hewes",
            "email": "aubrey.hewes@synetic.nl"
        },
        {
            "name": "Sander Jonk",
            "email": "sander.jonk@synetic.nl"
        },
        {
            "name": "Robin Speekenbrink",
            "email": "robin.speekenbrink@synetic.nl"
        },
        {
            "name": "Misha van Tol",
            "email": "misha.vantol@synetic.nl"
        },
        {
            "name": "Jordy Vleugel",
            "email": "jordy@synetic.nl"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "spatie/laravel-package-tools": "^1.13"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.9",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ray": "^1.26"
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Synetic\\Migator\\MigatorServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    }
}
